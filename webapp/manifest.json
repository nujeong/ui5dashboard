{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "ysc.websocket",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"init_data": {
				"uri": "model/init_data.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "ysc.websocket.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ysc.websocket.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "init_data"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "ysc.websocket.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"viewType": "XML",
				"transition": "slide"
			},
			"routes": [{
				"name": "Step01",
				"pattern": "Step01",
				"greedy": false,
				"target": "STEP01"
			}, {
				"name": "Step02",
				"pattern": "Step02",
				"greedy": false,
				"target": "STEP02"
			}, {
				"name": "SocketSender",
				"pattern": "SocketSender",
				"greedy": false,
				"target": "SOCKETSENDER"
			}],
			"targets": {
				"STEP01": {
					"viewName": "Step01",
					"viewId": "idStep01"
				},
				"STEP02": {
					"viewName": "Step02",
					"viewId": "idStep02"
				},
				"SOCKETSENDER": {
					"viewName": "SocketSender",
					"viewId": "idSocketSender"
				}
			}
		}
	}
}